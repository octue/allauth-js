{
  "name": "allauth-js",
  "version": "0.0.0-semantically-released",
  "description": "Allauth JS - using django allauth in headless mode for SPA frontends",
  "license": "GPL-3.0-only",
  "author": "Octue Ltd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octue/allauth-js.git"
  },
  "bugs": {
    "url": "https://github.com/octue/allauth-js/issues"
  },
  "homepage": "https://github.com/octue/allauth-js#readme",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "yarn run clean && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --reporter=github .",
    "format": "biome format --write .",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "allauth",
    "react",
    "authentication",
    "typescript",
    "vite",
    "library"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "v${version}",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "FEA",
              "release": "minor"
            },
            {
              "type": "ENH",
              "release": "patch"
            },
            {
              "type": "FIX",
              "release": "patch"
            },
            {
              "type": "OPS",
              "release": "patch"
            },
            {
              "type": "DEP",
              "release": "patch"
            },
            {
              "type": "REF",
              "release": "patch"
            },
            {
              "type": "TST",
              "release": "patch"
            },
            {
              "type": "MRG",
              "release": "patch"
            },
            {
              "type": "REV",
              "release": "patch"
            },
            {
              "type": "CHO",
              "release": "patch"
            },
            {
              "type": "WIP",
              "release": "patch"
            },
            {
              "type": "DOC",
              "release": "patch"
            },
            {
              "type": "STY",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "packageManager": "pnpm@8.15.1+sha1.8adba2d20330c02d3856e18c4eb3819d1d3ca6aa"
}
