{
  "name": "@octue/allauth-js",
  "version": "0.0.0-semantically-released",
  "description": "Allauth JS - using django allauth in headless mode for SPA frontends",
  "license": "GPL-3.0-only",
  "author": "Octue Ltd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octue/allauth-js.git"
  },
  "bugs": {
    "url": "https://github.com/octue/allauth-js/issues"
  },
  "homepage": "https://github.com/octue/allauth-js#readme",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "src/tailwind-preset.js"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "import": "./dist/nextjs/index.mjs",
      "require": "./dist/nextjs/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs"
    },
    "./tracking": {
      "types": "./dist/tracking/index.d.ts",
      "import": "./dist/tracking/index.mjs",
      "require": "./dist/tracking/index.cjs"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind-preset": "./src/tailwind-preset.js"
  },
  "typesVersions": {
    "*": {
      "core": [
        "dist/core/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ],
      "nextjs": [
        "dist/nextjs/index.d.ts"
      ],
      "tracking": [
        "dist/tracking/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --reporter=github .",
    "format": "biome format --write .",
    "prepublishOnly": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "next": "^15.5.4",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.22",
    "clsx": "^2.1.1",
    "material-ripple-effects": "^2.0.1",
    "rimraf": "^6.0.1",
    "storybook": "^10.0.8",
    "tailwindcss": "^3.4.18",
    "typescript": "5.8.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "allauth",
    "react",
    "authentication",
    "typescript",
    "vite",
    "library"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "${version}",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "FEA",
              "release": "minor"
            },
            {
              "type": "ENH",
              "release": "patch"
            },
            {
              "type": "FIX",
              "release": "patch"
            },
            {
              "type": "OPS",
              "release": "patch"
            },
            {
              "type": "DEP",
              "release": "patch"
            },
            {
              "type": "REF",
              "release": "patch"
            },
            {
              "type": "TST",
              "release": "patch"
            },
            {
              "type": "MRG",
              "release": "patch"
            },
            {
              "type": "REV",
              "release": "patch"
            },
            {
              "type": "CHO",
              "release": "patch"
            },
            {
              "type": "WIP",
              "release": "patch"
            },
            {
              "type": "DOC",
              "release": "patch"
            },
            {
              "type": "STY",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "packageManager": "pnpm@8.15.1+sha1.8adba2d20330c02d3856e18c4eb3819d1d3ca6aa",
  "dependencies": {
    "react-hook-form": "^7.63.0"
  }
}
